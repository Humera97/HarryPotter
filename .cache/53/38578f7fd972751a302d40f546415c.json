r.js","name":"console","original":{"line":40,"column":16},"generated":{"line":1,"column":1721}},{"source":"scripts/gender.js","name":"log","original":{"line":40,"column":24},"generated":{"line":1,"column":1729}},{"source":"scripts/gender.js","original":{"line":40,"column":28},"generated":{"line":1,"column":1733}},{"source":"scripts/gender.js","name":"err","original":{"line":40,"column":41},"generated":{"line":1,"column":1745}}],"sources":{"scripts/gender.js":"import * as d3 from 'd3'\n\nvar margin = { top: 30, left: 30, right: 100, bottom: 30 }\n\nvar height = 400 - margin.top - margin.bottom\n\nvar width = 780 - margin.left - margin.right\n\nvar svg = d3\n  .select('#gender')\n  .append('svg')\n  .attr('height', height + margin.top + margin.bottom)\n  .attr('width', width + margin.left + margin.right)\n  .append('g')\n  .attr('transform', 'translate(' + margin.left + ',' + margin.top + ')')\n\nvar colorScale = d3.scaleOrdinal().range([ '#879AD6','#7181B4','#546085', '#C1C8D7'])\nvar xPositionScale = d3\n  .scalePoint()\n  .domain([\"Sorceror's Stone\", 'Chamber of Secrets', \"Prisoner of Azkaban\", 'Total'])\n  .range([0, width])\n  .padding(0.4)\n\nvar arc = d3\n  .arc()\n  .innerRadius(0)\n  .outerRadius(80)\n\nvar pie = d3\n  .pie()\n  .value(function(d) {\n    return d.Lines\n  })\n  .sort(function(d1, d2) {\n    return d1.Gender.localeCompare(d2.Gender)\n  })\n\nd3.csv(require('/data/gender_lines.csv'))\n  .then(ready)\n  .catch(err => console.log('Failed on', err))\n\nfunction ready(datapoints) {\n  var nested = d3\n    .nest()\n    .key(function(d) {\n      return d.Movie\n    })\n    .entries(datapoints)\n\n  var charts = svg\n    .selectAll('g')\n    .data(nested)\n    .enter()\n    .append('g')\n    .attr('transform', function(d) {\n      var xPos = xPositionScale(d.key)\n      return 'translate(' + xPos + ',' + height / 2 + ')'\n    })\n\n  charts.each(function(d) {\n    d3.select(this)\n      .selectAll('path')\n      .data(pie(d.values))\n      .enter()\n      .append('path')\n      .attr('d', arc)\n      .attr('fill', function(d) {\n        console.log(d)\n        return colorScale(d.data.Gender)\n      })\n\n    d3.select(this)\n      .append('text')\n      .attr('color', '#CBD2E2')\n      .attr('y', 120)\n      .text(d.key)\n      .attr('text-anchor', 'middle')\n      .attr('font-family', 'Averia Libre')\n     .attr('font-weight', '800')\n      .attr('font-size', '16')\n      \n  })\n  \n}\n"},"lineCount":null}},"error":null,"hash":"7d6221e1ec0aa5002b8c96e836a6f5aa","cacheData":{"env":{}}}